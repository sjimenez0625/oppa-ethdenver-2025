{
  "name": "nestjs-starter-rest-api",
  "private": true,
  "version": "1.0.0",
  "description": "NestJS Starter REST API Repository",
  "license": "MIT",
  "contributors": [
    "Yash Murty <yashmurty@gmail.com> (https://github.com/yashmurty)",
    "S M Asad Rahman<asad.rahman@monstar-lab.com> (https://github.com/asad-mlbd)",
    "Tanveer Hassan <mailtanveerhassan@gmail.com> (https://github.com/war1oc)",
    "Saad Bin Amjad <saadbinamjad@gmail.com> (https://github.com/Saad-Amjad)",
    "Sivan Payyadakath <sivanp77@gmail.com > (https://github.com/sivanpayyadakath)"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && npm run sentry:sourcemaps",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "cli:dev": "ts-node -r tsconfig-paths/register src/cli.ts",
    "cli:prod": "node dist/src/cli.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "typeorm": "typeorm-ts-node-commonjs -d ./database/ormconfig.ts",
    "migration:generate": "ts-node node_modules/.bin/typeorm migration:generate -d database/ormconfig.ts database/migrations/$npm_config_name",
    "migration:run": "ts-node node_modules/.bin/typeorm migration:run -d database/ormconfig.ts",
    "migration:revert": "ts-node node_modules/.bin/typeorm migration:revert -d database/ormconfig.ts",
    "migration:generate:win": "ts-node node_modules/typeorm/cli.js migration:generate -d database/ormconfig.ts database/migrations/$npm_config_name",
    "migration:run:win": "ts-node node_modules/typeorm/cli.js migration:run -d database/ormconfig.ts",
    "migration:revert:win": "ts-node node_modules/typeorm/cli.js migration:revert -d database/ormconfig.ts",
    "seed:sample": "ts-node database/seeds/sample.seeder.ts",
    "doc:serve": "npx compodoc -p tsconfig.json -w -s -r 8001",
    "doc:generate": "npx compodoc -p tsconfig.json",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org trova-wj --project trova-core ./dist && sentry-cli sourcemaps upload --org trova-wj --project trova-core ./dist"
  },
  "lint-staged": {
    "**/*.ts": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.1.3",
    "@aws-sdk/client-s3": "^3.734.0",
    "@aws-sdk/client-ses": "^3.734.0",
    "@aws-sdk/lib-storage": "^3.734.0",
    "@aws-sdk/s3-request-presigner": "^3.734.0",
    "@dataui/crud": "^5.3.4",
    "@dataui/crud-request": "^5.3.4",
    "@dataui/crud-typeorm": "^5.3.4",
    "@dfinity/identity": "^2.3.0",
    "@dfinity/ledger-icp": "^2.6.9",
    "@dfinity/utils": "^2.10.1",
    "@keyv/redis": "^4.2.0",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/cache-manager": "^3.0.0",
    "@nestjs/common": "^11.0.5",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.5",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.5",
    "@nestjs/platform-socket.io": "^11.0.5",
    "@nestjs/swagger": "^11.0.3",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.0.5",
    "@sentry/cli": "^2.41.1",
    "@sentry/nestjs": "^8.51.0",
    "@sentry/profiling-node": "^8.51.0",
    "@types/uuid": "^10.0.0",
    "bcrypt": "^5.1.1",
    "cache-manager": "^6.4.0",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "crc-32": "^1.2.2",
    "dotenv": "^16.4.7",
    "joi": "^17.13.3",
    "jwks-rsa": "^3.1.0",
    "lodash": "^4.17.21",
    "newrelic": "^12.11.2",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-auth0": "^1.4.4",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.13.1",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "5.0.1",
    "typeorm": "^0.3.20",
    "uuid": "^11.0.5",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@compodoc/compodoc": "^1.1.26",
    "@eslint/js": "^9.18.0",
    "@faker-js/faker": "^9.4.0",
    "@nestjs/cli": "^11.0.2",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.5",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.15",
    "@types/node": "^22.10.10",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-auth0": "^1.0.9",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.11.11",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^15.4.2",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm-extension": "^3.6.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0"
  },
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coveragePathIgnorePatterns": [
      ".module.ts$"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
